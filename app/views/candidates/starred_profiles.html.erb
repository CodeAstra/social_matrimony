<% if @stared_array.count == 0 %>
  <h3>Oops..You havent stared any profiles yet!!</h3>
<% else %>
  <% @matches.each do |match| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
        <div class="caption">
          <h3 class="text-center"><%= match.name %></h3>
        </div>
      </div>
    </div>
  <% end %>
<% end %>