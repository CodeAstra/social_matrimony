<% if @ignored_array.count == 0 %>
  <h3>Oops..You havent ignored any profiles yet!!</h3>
<% else %>
  <% @ignored_array.each do |id| %>
    <% @matches.each do |match| %>
      <% if match.id == id %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <div class="caption">
              <h3 class="text-center"><%= match.name %></h3>
            </div>
          </div>
        </div>
      <% end %>
    <% end %>
  <% end %>
<% end %>